{
  "host": "api.deniapps.com",
  "port": "PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mongodb": "mongodb://localhost:27017/deniUser",
  "mongoUser": "user",
  "mongoPass": "pass",
  "authentication": {
    "secret": "",
    "strategies": ["jwt", "local"],
    "path": "/authentication",
    "service": "users",
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://deniapps.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}
